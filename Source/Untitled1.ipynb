    "from vpython import *\n",
    "L=W=10\n",
    "l=1\n",
    "Ex=Hy=1\n",
    "\n",
    "grids=[]\n",
    "\n",
    "\n",
    "for i in range(L):\n",
    "\tline=[]\n",
    "\tfor j in range(W):\n",
    "\t\tline.append(box(pos=vector(l*i,l*j,l*0), length=l, height=l, width=0,color=vector(Ex,Hy,0)))\n",
    "\tgrids.append(line)\n",
    "\t\n",
    "    \n",
    "lines=grids[2]\n",
    "lines[2].color=vector(3,2,0)"
